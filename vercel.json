{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@now/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "dist/main.js"
      }
    ],
    "scripts": {
      "build": "npm run build",
      "dev": "npm run start",
      "prod": "npm run start:prod"
    }
  }